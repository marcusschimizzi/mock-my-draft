{
  "name": "draft-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/draft-api/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node -r tsconfig-paths/register apps/draft-api/src/commands/seed.ts",
        "envFile": "apps/draft-api/.env"
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "draft-api:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "draft-api:build:development"
        },
        "production": {
          "buildTarget": "draft-api:build:production"
        }
      }
    }
  }
}
